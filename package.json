{
  "name": "surreal",
  "version": "1.0.0",
  "description": "Electron + ThreeVRM AI Galgame Framework",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:build-main": "tsc -p tsconfig.electron.json && tsc -p tsconfig.preload.json",
    "electron:dev": "cross-env NODE_ENV=development pnpm run electron:build-main && electron .",
    "electron:start": "concurrently \"pnpm run dev\" \"wait-on http://localhost:5173 && pnpm run electron:dev\"",
    "electron:build": "npm run build && electron-builder"
  },
  "keywords": [
    "electron",
    "three.js",
    "vrm",
    "galgame"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/three": "^0.182.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "@pixiv/three-vrm": "^3.4.4",
    "@pixiv/three-vrm-materials-mtoon": "^3.4.4",
    "three": "^0.182.0",
    "three-fbx-loader": "^1.0.3"
  }
}
